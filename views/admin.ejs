<%- include("./partials/header") %>
    <div class="container">
        <div class="jumbotron">
            <h1 style="font-size:50px ;">All The Owner and Tenant details are given below </h1><br>
            <a class="btn btn-info btn-lg" href="/Admin/addOwner/<%=currentuser._id%>">Add Owner</a>
        </div>
        <div style="width:75%; margin:0 auto;">
            <h1 class="text-center">Owner Details</h1>
            <table class="table table-bordered"> 
                <tr>
                    <th>Username</th>
                    <th>Email ID </th>
                    <th>Phone Number</th>
                    <th>Remove Owner</th>
                </tr>
                <% owners.forEach(function(data){%>
                    <tr>
                        <td><%=data.username%></td>
                        <td><%=data.email%></td>
                        <td><%=data.phoneNo%></td>
                        <td><a class="btn btn-danger btn-sm" href="/removeOwner/<%=currentuser._id%>">Delete</a></td>
                    </tr>
                <%})%>
            </table>
        </div>
    </div>
<%- include("./partials/footer") %>
