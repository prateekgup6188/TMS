<%- include("partials/header") %>
    <div class="container">
        <div class="Jumbotron" style="margin:25px 0px 25px 0px; background-color:whitesmoke; padding: inherit;">
            <h1>Welcome To Tenant Management System</h1><br><br>
            <p style="font-size:20px">Following are the properties that belong to this owner!! </p>
        </div>
        <div class="row">
            <%property.forEach(function(data){%>
                <div class="col-md-4">
                    <div class="thumbnail">
                        <img src="<%=data.image%>">
                        <div class="caption">
                            <p style="text-align: center;"><a href="/owner/<%=owner_id%>/property/<%=data._id%>" class="btn btn-primary" role="button">Show More...</a></p>
                        </div>
                    </div>
                </div>
            <%})%>
        </div>
    </div>
<%- include("partials/footer") %>