<%- include("./partials/footer") %>
    <div class="container">
        <div class="row">
            <div class ="col-md-12 text-center">
                <h1>Edit Property</h1>
            </div>
            <div style="width:30%; margin:30px auto;">
                <form action="/addproperty/<%=currentowner._id%>" method="POST">
                    <div class="form-group">
                        <input class="form-control" type="number" name="Property[rooms]" value="<%=prop.rooms%>">
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="text" name="Property[status]" value="prop.status">
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="text" name="Property[address]"  value="prop.address">
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="number" name="Property[rent]" value="prop.rent">
                    </div>
                    <div class="form-group">
                        <button class="btn btn-md btn-info btn-block">Submit</button>
                    </div>
                </form>
                <a class="btn btn-primary" href="/owner">Back</a>
            </div>
        </div>
    </div>
<%- include("./partials/footer") %>
